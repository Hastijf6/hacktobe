import time,os,sys

cases = [
  {"name": "School Mystery", 
  "case number": "1"},
  {"name": "Science Lab",
  "case number": "2"}]
  
for case in cases:
  total_case = len(case)
  

def typingPrint(text):
  for char in text:
    sys.stdout.write(char)
    sys.stdout.flush()
    time.sleep(0.05)
  
def typingInput(text):
  for char in text:
    sys.stdout.write(char)
    sys.stdout.flush()
    time.sleep(0.05)
  value = input()  
  return value  

    
typingPrint("Hi Detective")
time.sleep(1)
print("")
typingPrint("You have " + str(total_case) + " remaining cases")
time.sleep(1)
print("")
print(cases)

